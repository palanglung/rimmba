<multi-tile-grid-{{ ai_gen_id }} class="ai-tile-grid-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <div class="ai-tile-grid__container-{{ ai_gen_id }}">
    {% for i in (1..9) %}
      {% liquid
        assign tile_enabled_key = 'tile_' | append: i | append: '_enabled'
        assign tile_page_key = 'tile_' | append: i | append: '_page'
        assign tile_url_key = 'tile_' | append: i | append: '_url'
        assign tile_title_key = 'tile_' | append: i | append: '_title'
        assign tile_description_key = 'tile_' | append: i | append: '_description'
        assign tile_image_key = 'tile_' | append: i | append: '_image'
        assign tile_heading_level_key = 'tile_' | append: i | append: '_heading_level'
        assign tile_title_size_key = 'tile_' | append: i | append: '_title_size'
        assign tile_description_size_key = 'tile_' | append: i | append: '_description_size'
        assign tile_new_tab_key = 'tile_' | append: i | append: '_new_tab'
        assign tile_button_text_key = 'tile_' | append: i | append: '_button_text'
        assign tile_button_style_key = 'tile_' | append: i | append: '_button_style'
        assign tile_button_align_key = 'tile_' | append: i | append: '_button_align'
        assign tile_color_scheme_key = 'tile_' | append: i | append: '_color_scheme'
        assign tile_content_align_key = 'tile_' | append: i | append: '_content_align'
        assign tile_overlay_key = 'tile_' | append: i | append: '_overlay'

        assign tile_enabled = block.settings[tile_enabled_key]
        assign tile_page = block.settings[tile_page_key]
        assign tile_url = block.settings[tile_url_key]
        assign tile_title = block.settings[tile_title_key]
        assign tile_description = block.settings[tile_description_key]
        assign tile_image = block.settings[tile_image_key]
        assign tile_heading_level = block.settings[tile_heading_level_key]
        assign tile_title_size = block.settings[tile_title_size_key]
        assign tile_description_size = block.settings[tile_description_size_key]
        assign tile_new_tab = block.settings[tile_new_tab_key]
        assign tile_button_text = block.settings[tile_button_text_key]
        assign tile_button_style = block.settings[tile_button_style_key]
        assign tile_button_align = block.settings[tile_button_align_key]
        assign tile_color_scheme = block.settings[tile_color_scheme_key]
        assign tile_content_align = block.settings[tile_content_align_key]
        assign tile_overlay = block.settings[tile_overlay_key]

        if tile_url != blank
          assign final_url = tile_url
        elsif tile_page
          assign final_url = tile_page.url
        else
          assign final_url = '#'
        endif

        if tile_title != blank
          assign final_title = tile_title
        elsif tile_page
          assign final_title = tile_page.title
        else
          assign final_title = ''
        endif

        if tile_description != blank
          assign final_description = tile_description
        elsif tile_page and tile_page.metafields.global.description_tag
          assign final_description = tile_page.metafields.global.description_tag
        else
          assign final_description = ''
        endif

        if tile_image
          assign final_image = tile_image
        elsif tile_page and tile_page.image
          assign final_image = tile_page.image
        else
          assign final_image = null
        endif

        assign target_attr = ''
        if tile_new_tab
          assign target_attr = 'target="_blank" rel="noopener noreferrer"'
        endif

        assign is_clickable = false
        if tile_button_text == blank and final_url != '#'
          assign is_clickable = true
        endif

        assign overlay_opacity = 0
        if tile_overlay == 'light'
          assign overlay_opacity = 0.2
        elsif tile_overlay == 'medium'
          assign overlay_opacity = 0.5
        elsif tile_overlay == 'dark'
          assign overlay_opacity = 0.7
        endif

        assign scheme = settings.color_schemes[tile_color_scheme].settings
      %}

      {% if tile_enabled %}
        {% if is_clickable %}
          <a
            href="{{ final_url }}"
            class="ai-tile-{{ ai_gen_id }} ai-tile-{{ ai_gen_id }}--clickable color-{{ tile_color_scheme }}"
            {{ target_attr }}
            style="color: {{ scheme.primary_text }};"
          >
        {% else %}
          <div class="ai-tile-{{ ai_gen_id }} color-{{ tile_color_scheme }}" style="color: {{ scheme.primary_text }};">
        {% endif %}

          {% if final_image %}
            <img
              src="{{ final_image | image_url: width: 1200 }}"
              alt="{{ final_image.alt | escape }}"
              class="ai-tile__image-{{ ai_gen_id }}"
              loading="lazy"
              width="{{ final_image.width }}"
              height="{{ final_image.height }}"
            >
          {% else %}
            <div class="ai-tile__placeholder-{{ ai_gen_id }}">
              {{ 'image' | placeholder_svg_tag }}
            </div>
          {% endif %}

          {% if tile_overlay != 'none' %}
            <div
              class="ai-tile__overlay-{{ ai_gen_id }}"
              style="background-color: {{ scheme.background }}; opacity: {{ overlay_opacity }};"
            ></div>
          {% endif %}

          <div class="ai-tile__content-{{ ai_gen_id }} ai-tile__content-{{ ai_gen_id }}--{{ tile_content_align }}">
            <div>
              {% if final_title != blank %}
                {% case tile_heading_level %}
                  {% when 'h2' %}
                    <h2 class="ai-tile__title-{{ ai_gen_id }} ai-tile__title-{{ ai_gen_id }}--{{ tile_title_size }}">
                      {{ final_title }}
                    </h2>
                  {% when 'h3' %}
                    <h3 class="ai-tile__title-{{ ai_gen_id }} ai-tile__title-{{ ai_gen_id }}--{{ tile_title_size }}">
                      {{ final_title }}
                    </h3>
                  {% when 'h4' %}
                    <h4 class="ai-tile__title-{{ ai_gen_id }} ai-tile__title-{{ ai_gen_id }}--{{ tile_title_size }}">
                      {{ final_title }}
                    </h4>
                  {% when 'h5' %}
                    <h5 class="ai-tile__title-{{ ai_gen_id }} ai-tile__title-{{ ai_gen_id }}--{{ tile_title_size }}">
                      {{ final_title }}
                    </h5>
                {% endcase %}
              {% endif %}

              {% if final_description != blank %}
                <p class="ai-tile__description-{{ ai_gen_id }} ai-tile__description-{{ ai_gen_id }}--{{ tile_description_size }}" style="color: {{ scheme.secondary_text }};">
                  {{ final_description }}
                </p>
              {% endif %}
            </div>

            {% if tile_button_text != blank %}
              <div class="ai-tile__button-wrapper-{{ ai_gen_id }} ai-tile__button-wrapper-{{ ai_gen_id }}--{{ tile_button_align }}">
                <a
                  href="{{ final_url }}"
                  class="ai-tile__button-{{ ai_gen_id }} ai-tile__button-{{ ai_gen_id }}--{{ tile_button_style }}"
                  {{ target_attr }}
                  {% if tile_button_style == 'filled' %}
                    style="background-color: {{ scheme.primary_button }}; color: {{ scheme.primary_button_label }};"
                  {% else %}
                    style="color: {{ scheme.primary_text }};"
                  {% endif %}
                >
                  {{ tile_button_text }}
                </a>
              </div>
            {% endif %}
          </div>

        {% if is_clickable %}
          </a>
        {% else %}
          </div>
        {% endif %}
      {% endif %}
    {% endfor %}
  </div>
</multi-tile-grid-{{ ai_gen_id }}>

<script>
  (function() {
    class MultiTileGrid{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
      }

      connectedCallback() {
        this.adjustGrid();
        window.addEventListener('resize', () => this.adjustGrid());
      }

      adjustGrid() {
        const container = this.querySelector('.ai-tile-grid__container-{{ ai_gen_id }}');
        const tiles = container.querySelectorAll('.ai-tile-{{ ai_gen_id }}');
        
        if (window.innerWidth < 750) {
          tiles.forEach(tile => {
            tile.style.gridColumn = '';
            tile.style.gridRow = '';
          });
        }
      }
    }

    customElements.define('multi-tile-grid-{{ ai_gen_id }}', MultiTileGrid{{ ai_gen_id }});
  })();
</script>

