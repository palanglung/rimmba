{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-tile-grid-{{ ai_gen_id }} {
    display: block;
    width: 100%;
  }

  .ai-tile-grid__container-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    gap: 16px;
  }

  @media screen and (min-width: 750px) {
    .ai-tile-grid__container-{{ ai_gen_id }} {
      grid-template-columns: repeat(3, 1fr);
    }

    .ai-tile-grid__container-{{ ai_gen_id }} .ai-tile-{{ ai_gen_id }}:first-child {
      grid-column: span 2;
      grid-row: span 2;
    }
  }

  .ai-tile-{{ ai_gen_id }} {
    position: relative;
    min-height: 300px;
    border-radius: {{ block.settings.tile_border_radius }}px;
    overflow: hidden;
    display: flex;
    flex-direction: column;
  }

  .ai-tile-{{ ai_gen_id }}--clickable {
    cursor: pointer;
    text-decoration: none;
    color: inherit;
  }

  .ai-tile__image-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .ai-tile__placeholder-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: #f4f4f4;
  }

  .ai-tile__placeholder-{{ ai_gen_id }} svg {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .ai-tile__overlay-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    pointer-events: none;
  }

  .ai-tile__content-{{ ai_gen_id }} {
    position: relative;
    z-index: 1;
    padding: {{ block.settings.tile_padding }}px;
    display: flex;
    flex-direction: column;
    height: 100%;
  }

  .ai-tile__content-{{ ai_gen_id }}--top {
    justify-content: flex-start;
  }

  .ai-tile__content-{{ ai_gen_id }}--center {
    justify-content: center;
  }

  .ai-tile__content-{{ ai_gen_id }}--bottom {
    justify-content: flex-end;
  }

  .ai-tile__content-{{ ai_gen_id }}--distributed {
    justify-content: space-between;
  }

  .ai-tile__title-{{ ai_gen_id }} {
    margin: 0 0 8px 0;
    font-family: {{ settings.type_heading_font.family }}, {{ settings.type_heading_font.fallback_families }};
    font-weight: {{ settings.type_heading_font.weight }};
    text-transform: {{ settings.heading_text_transform }};
    letter-spacing: {{ settings.heading_letter_spacing }};
  }

  .ai-tile__title-{{ ai_gen_id }}--s {
    font-size: calc(1.6rem * {{ settings.heading_font_scale | divided_by: 100.0 }});
  }

  .ai-tile__title-{{ ai_gen_id }}--m {
    font-size: calc(2.4rem * {{ settings.heading_font_scale | divided_by: 100.0 }});
  }

  .ai-tile__title-{{ ai_gen_id }}--l {
    font-size: calc(3.2rem * {{ settings.heading_font_scale | divided_by: 100.0 }});
  }

  .ai-tile__title-{{ ai_gen_id }}--xl {
    font-size: calc(4.8rem * {{ settings.heading_font_scale | divided_by: 100.0 }});
  }

  .ai-tile__description-{{ ai_gen_id }} {
    margin: 0;
    font-family: {{ settings.type_body_font.family }}, {{ settings.type_body_font.fallback_families }};
    font-weight: {{ settings.type_body_font.weight }};
  }

  .ai-tile__description-{{ ai_gen_id }}--s {
    font-size: calc(1.2rem * {{ settings.body_font_scale | divided_by: 100.0 }});
  }

  .ai-tile__description-{{ ai_gen_id }}--m {
    font-size: calc(1.4rem * {{ settings.body_font_scale | divided_by: 100.0 }});
  }

  .ai-tile__description-{{ ai_gen_id }}--l {
    font-size: calc(1.6rem * {{ settings.body_font_scale | divided_by: 100.0 }});
  }

  .ai-tile__description-{{ ai_gen_id }}--xl {
    font-size: calc(2rem * {{ settings.body_font_scale | divided_by: 100.0 }});
  }

  .ai-tile__button-wrapper-{{ ai_gen_id }} {
    margin-top: 16px;
  }

  .ai-tile__button-wrapper-{{ ai_gen_id }}--left {
    text-align: left;
  }

  .ai-tile__button-wrapper-{{ ai_gen_id }}--center {
    text-align: center;
  }

  .ai-tile__button-wrapper-{{ ai_gen_id }}--right {
    text-align: right;
  }

  .ai-tile__button-wrapper-{{ ai_gen_id }}--full .ai-tile__button-{{ ai_gen_id }} {
    width: 100%;
  }

  .ai-tile__button-{{ ai_gen_id }} {
    display: inline-block;
    padding: 12px 24px;
    text-decoration: none;
    font-family: {{ settings.type_button_font.family }}, {{ settings.type_button_font.fallback_families }};
    font-weight: {{ settings.type_button_font.weight }};
    font-size: calc(1.4rem * {{ settings.button_font_scale | divided_by: 100.0 }});
    letter-spacing: {{ settings.button_letter_spacing }};
    text-transform: {{ settings.button_text_transform }};
    border-radius: {{ settings.button_border_radius }};
    transition: all 0.3s ease;
  }

  .ai-tile__button-{{ ai_gen_id }}--filled {
    border: 2px solid transparent;
  }

  .ai-tile__button-{{ ai_gen_id }}--outline {
    background: transparent;
    border: 2px solid currentColor;
  }

  .ai-tile__button-{{ ai_gen_id }}--text {
    background: transparent;
    border: none;
    text-decoration: underline;
    padding: 0;
  }

  @media screen and (max-width: 749px) {
    .ai-tile-grid__container-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
    }

    .ai-tile-{{ ai_gen_id }} {
      min-height: 250px;
    }
  }
