{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% liquid
  case block.settings.container_width
    when 'narrow'
      assign container_max_width = '800px'
    when 'fluid'
      assign container_max_width = '1200px'
    when 'full'
      assign container_max_width = '100%'
  endcase

  case block.settings.container_height
    when 'auto'
      assign container_height = 'auto'
    when 'third'
      assign container_height = '33vh'
    when 'half'
      assign container_height = '50vh'
    when 'full'
      assign container_height = '100vh'
  endcase

  case block.settings.spacing_top
    when 'no'
      assign spacing_top = '0'
    when 's'
      assign spacing_top = '20px'
    when 'm'
      assign spacing_top = '40px'
    when 'l'
      assign spacing_top = '60px'
    when 'xl'
      assign spacing_top = '80px'
  endcase

  case block.settings.spacing_bottom
    when 'no'
      assign spacing_bottom = '0'
    when 's'
      assign spacing_bottom = '20px'
    when 'm'
      assign spacing_bottom = '40px'
    when 'l'
      assign spacing_bottom = '60px'
    when 'xl'
      assign spacing_bottom = '80px'
  endcase

  case block.settings.tiles_spacing
    when 'no'
      assign tiles_gap = '0'
    when 's'
      assign tiles_gap = '10px'
    when 'm'
      assign tiles_gap = '20px'
    when 'l'
      assign tiles_gap = '30px'
    when 'xl'
      assign tiles_gap = '40px'
  endcase
%}

{% style %}
  .ai-multi-tile-container-{{ ai_gen_id }} {
    display: block;
    width: 100%;
    padding-top: {{ spacing_top }};
    padding-bottom: {{ spacing_bottom }};
  }

  .ai-multi-tile-wrapper-{{ ai_gen_id }} {
    max-width: {{ container_max_width }};
    margin: 0 auto;
    padding: 0 20px;
  }

  .ai-multi-tile-header-{{ ai_gen_id }} {
    margin-bottom: 40px;
    text-align: {{ block.settings.header_alignment }};
  }

  .ai-multi-tile-title-{{ ai_gen_id }} {
    margin: 0 0 16px 0;
  }

  .ai-multi-tile-description-{{ ai_gen_id }} {
    margin: 0;
  }

  .ai-multi-tile-grid-{{ ai_gen_id }} {
    display: grid;
    gap: {{ tiles_gap }};
    grid-template-columns: repeat({{ block.settings.columns }}, 1fr);
    grid-template-rows: repeat({{ block.settings.rows }}, 1fr);
    {% if block.settings.container_height != 'auto' %}
      height: {{ container_height }};
    {% endif %}
  }

  {% if block.settings.masonry_enabled and block.settings.columns > 1 %}
    .ai-multi-tile-grid-{{ ai_gen_id }} .ai-multi-tile-item-{{ ai_gen_id }}:first-child {
      grid-row: 1 / span {{ block.settings.rows }};
    }
  {% endif %}

  .ai-multi-tile-item-{{ ai_gen_id }} {
    position: relative;
    overflow: hidden;
    {% if block.settings.container_height == 'auto' %}
      min-height: 300px;
    {% endif %}
    display: flex;
    border-radius: {{ settings.card_corner_radius }};
  }

  .ai-multi-tile-link-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 3;
    text-decoration: none;
  }

  .ai-multi-tile-bg-wrapper-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 0;
  }

  .ai-multi-tile-bg-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .ai-multi-tile-content-{{ ai_gen_id }} {
    position: relative;
    z-index: 2;
    width: 100%;
    display: flex;
    flex-direction: column;
  }

  .ai-multi-tile-content-top-{{ ai_gen_id }} {
    justify-content: flex-start;
  }

  .ai-multi-tile-content-center-{{ ai_gen_id }} {
    justify-content: center;
  }

  .ai-multi-tile-content-bottom-{{ ai_gen_id }} {
    justify-content: flex-end;
  }

  .ai-multi-tile-content-align-start-{{ ai_gen_id }} {
    align-items: flex-start;
    text-align: left;
  }

  .ai-multi-tile-content-align-center-{{ ai_gen_id }} {
    align-items: center;
    text-align: center;
  }

  .ai-multi-tile-content-align-end-{{ ai_gen_id }} {
    align-items: flex-end;
    text-align: right;
  }

  .ai-multi-tile-inner-{{ ai_gen_id }} {
    width: 100%;
  }

  .ai-multi-tile-item-title-{{ ai_gen_id }} {
    margin: 0 0 12px 0;
  }

  .ai-multi-tile-item-description-{{ ai_gen_id }} {
    margin: 0 0 20px 0;
  }

  .ai-multi-tile-button-{{ ai_gen_id }} {
    position: relative;
    z-index: 4;
    display: inline-block;
    padding: 12px 24px;
    text-decoration: none;
    border-radius: {{ settings.button_border_radius }};
    transition: all 0.3s ease;
    cursor: pointer;
    font-weight: 500;
    letter-spacing: {{ settings.button_letter_spacing }};
    text-transform: {{ settings.button_text_transform }};
    border: 2px solid;
  }

  {% for i in (1..9) %}
    {% liquid
      assign tile_color_scheme_key = 'tile_' | append: i | append: '_color_scheme'
      assign tile_scheme = block.settings[tile_color_scheme_key]
    %}
    
    .ai-multi-tile-button-filled-{{ ai_gen_id }}-{{ i }} {
      background-color: rgba(var(--color-button), 1);
      color: rgba(var(--color-button-text), 1);
      border-color: rgba(var(--color-button), 1);
    }

    .ai-multi-tile-button-filled-{{ ai_gen_id }}-{{ i }}:hover {
      opacity: 0.85;
    }

    .ai-multi-tile-button-outlined-{{ ai_gen_id }}-{{ i }} {
      background-color: transparent;
      color: rgba(var(--color-foreground), 1);
      border-color: rgba(var(--color-foreground), 1);
    }

    .ai-multi-tile-button-outlined-{{ ai_gen_id }}-{{ i }}:hover {
      background-color: rgba(var(--color-button), 1);
      color: rgba(var(--color-button-text), 1);
      border-color: rgba(var(--color-button), 1);
    }

    .ai-multi-tile-button-text-{{ ai_gen_id }}-{{ i }} {
      background: transparent;
      border: none;
      padding: 12px 0;
      color: rgba(var(--color-foreground), 1);
      text-decoration: underline;
    }

    .ai-multi-tile-button-text-{{ ai_gen_id }}-{{ i }}:hover {
      opacity: 0.7;
    }
  {% endfor %}

  @media screen and (max-width: 768px) {
    .ai-multi-tile-grid-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
      grid-template-rows: auto;
      height: auto;
    }

    {% if block.settings.masonry_enabled %}
      .ai-multi-tile-grid-{{ ai_gen_id }} .ai-multi-tile-item-{{ ai_gen_id }}:first-child {
        grid-row: auto;
      }
    {% endif %}

    .ai-multi-tile-item-{{ ai_gen_id }} {
      min-height: 300px;
    }
  }
{% endstyle %}

<multi-tile-container-{{ ai_gen_id }}
  class="ai-multi-tile-container-{{ ai_gen_id }} color-{{ block.settings.color_scheme }}"
  {{ block.shopify_attributes }}
>
  <div class="ai-multi-tile-wrapper-{{ ai_gen_id }}">
    {% if block.settings.title != blank or block.settings.description != blank %}
      <div class="ai-multi-tile-header-{{ ai_gen_id }}">
        {% if block.settings.title != blank %}
          {% case block.settings.title_tag %}
            {% when 'h2' %}
              <h2 class="ai-multi-tile-title-{{ ai_gen_id }}">{{ block.settings.title }}</h2>
            {% when 'h3' %}
              <h3 class="ai-multi-tile-title-{{ ai_gen_id }}">{{ block.settings.title }}</h3>
            {% when 'h4' %}
              <h4 class="ai-multi-tile-title-{{ ai_gen_id }}">{{ block.settings.title }}</h4>
            {% when 'h5' %}
              <h5 class="ai-multi-tile-title-{{ ai_gen_id }}">{{ block.settings.title }}</h5>
          {% endcase %}
        {% endif %}
        {% if block.settings.description != blank %}
          <div class="ai-multi-tile-description-{{ ai_gen_id }}">{{ block.settings.description }}</div>
        {% endif %}
      </div>
    {% endif %}

    <div class="ai-multi-tile-grid-{{ ai_gen_id }}">
      {% for i in (1..9) %}
        {% if i <= block.settings.tiles_to_show %}
          {% liquid
            assign tile_color_scheme_key = 'tile_' | append: i | append: '_color_scheme'
            assign page_key = 'tile_' | append: i | append: '_page'
            assign custom_url_key = 'tile_' | append: i | append: '_custom_url'
            assign open_new_tab_key = 'tile_' | append: i | append: '_open_new_tab'
            assign tile_is_link_key = 'tile_' | append: i | append: '_tile_is_link'
            assign custom_title_key = 'tile_' | append: i | append: '_custom_title'
            assign custom_description_key = 'tile_' | append: i | append: '_custom_description'
            assign button_label_key = 'tile_' | append: i | append: '_button_label'
            assign button_style_key = 'tile_' | append: i | append: '_button_style'
            assign background_image_key = 'tile_' | append: i | append: '_background_image'
            assign overlay_strength_key = 'tile_' | append: i | append: '_overlay_strength'
            assign content_position_key = 'tile_' | append: i | append: '_content_position'
            assign content_alignment_key = 'tile_' | append: i | append: '_content_alignment'
            assign tile_padding_key = 'tile_' | append: i | append: '_tile_padding'

            assign tile_color_scheme = block.settings[tile_color_scheme_key]
            assign tile_page = block.settings[page_key]
            assign tile_url = block.settings[custom_url_key]
            
            if tile_page != blank and tile_url == blank
              assign tile_url = tile_page.url
            endif

            assign tile_title = block.settings[custom_title_key]
            if tile_title == blank and tile_page != blank
              assign tile_title = tile_page.title
            endif

            assign tile_description = block.settings[custom_description_key]
            if tile_description == blank and tile_page != blank
              assign tile_description = tile_page.content | strip_html | truncate: 150
            endif

            assign tile_image = block.settings[background_image_key]
            if tile_image == blank and tile_page != blank and tile_page.image != blank
              assign tile_image = tile_page.image
            endif

            assign overlay_strength = block.settings[overlay_strength_key]
            case overlay_strength
              when 'no'
                assign overlay_opacity = '1'
              when 'soft'
                assign overlay_opacity = '0.7'
              when 'medium'
                assign overlay_opacity = '0.4'
              when 'strong'
                assign overlay_opacity = '0.15'
            endcase

            assign tile_padding_value = block.settings[tile_padding_key]
            case tile_padding_value
              when 'no'
                assign tile_padding = '0'
              when 's'
                assign tile_padding = '20px'
              when 'm'
                assign tile_padding = '30px'
              when 'l'
                assign tile_padding = '40px'
              when 'xl'
                assign tile_padding = '50px'
            endcase

            assign target_attr = ''
            if block.settings[open_new_tab_key]
              assign target_attr = 'target="_blank" rel="noopener noreferrer"'
            endif

            assign content_position = block.settings[content_position_key]
            assign content_alignment = block.settings[content_alignment_key]
            assign button_label = block.settings[button_label_key]
            assign button_style = block.settings[button_style_key]
            assign tile_is_link = block.settings[tile_is_link_key]
          %}

          <div class="ai-multi-tile-item-{{ ai_gen_id }} color-{{ tile_color_scheme }}">
            {% if tile_is_link and tile_url != blank %}
              <a
                href="{{ tile_url }}"
                class="ai-multi-tile-link-{{ ai_gen_id }}"
                {{ target_attr }}
                aria-label="{{ tile_title | escape }}"
              ></a>
            {% endif %}

            {% if tile_image %}
              <div class="ai-multi-tile-bg-wrapper-{{ ai_gen_id }}" style="opacity: {{ overlay_opacity }};">
                <img
                  src="{{ tile_image | image_url: width: 1200 }}"
                  alt="{{ tile_image.alt | escape }}"
                  class="ai-multi-tile-bg-{{ ai_gen_id }}"
                  loading="lazy"
                >
              </div>
            {% endif %}

            <div
              class="ai-multi-tile-content-{{ ai_gen_id }} ai-multi-tile-content-{{ content_position }}-{{ ai_gen_id }} ai-multi-tile-content-align-{{ content_alignment }}-{{ ai_gen_id }}"
              style="padding: {{ tile_padding }};"
            >
              <div class="ai-multi-tile-inner-{{ ai_gen_id }}">
                {% if tile_title != blank %}
                  <h3 class="ai-multi-tile-item-title-{{ ai_gen_id }}">
                    {{ tile_title }}
                  </h3>
                {% endif %}

                {% if tile_description != blank %}
                  <div class="ai-multi-tile-item-description-{{ ai_gen_id }}">
                    {{ tile_description }}
                  </div>
                {% endif %}

                {% if button_label != blank %}
                  <a
                    href="{{ tile_url }}"
                    class="ai-multi-tile-button-{{ ai_gen_id }} ai-multi-tile-button-{{ button_style }}-{{ ai_gen_id }}-{{ i }}"
                    {{ target_attr }}
                  >
                    {{ button_label }}
                  </a>
                {% endif %}
              </div>
            </div>
          </div>
        {% endif %}
      {% endfor %}
    </div>
  </div>
</multi-tile-container-{{ ai_gen_id }}>

<script>
  (function() {
    class MultiTileContainer{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
      }

      connectedCallback() {
        this.initializeTiles();
      }

      initializeTiles() {
        const tiles = this.querySelectorAll('.ai-multi-tile-item-{{ ai_gen_id }}');
        tiles.forEach(tile => {
          tile.addEventListener('mouseenter', () => {
            tile.style.transform = 'scale(1.02)';
            tile.style.transition = 'transform 0.3s ease';
          });
          tile.addEventListener('mouseleave', () => {
            tile.style.transform = 'scale(1)';
          });
        });
      }
    }

    customElements.define('multi-tile-container-{{ ai_gen_id }}', MultiTileContainer{{ ai_gen_id }});
  })();
</script>

